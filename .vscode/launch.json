{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "mock",
            "request": "launch",
            "name": "Mock debug",
            "program": "${workspaceFolder}/package.json",
            "stopOnEntry": true
        },
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Launch Extension",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm",
            "env": {
                //"NODE_DEBUG": "NET,CHILD_PROCESS,GFS4,STREAM,TIMER"
            }
        },
        {
            "type": "qore",
            "request": "launch",
            "name": "Launch test-binary.q",
            "program": "${workspaceFolder}/test-binary.q"
        },
        {
            "type": "qore",
            "request": "launch",
            "name": "Launch test-basic.q",
            "program": "${workspaceFolder}/test/test-basic.q"
        },
        {
            "type": "qore",
            "request": "launch",
            "name": "Launch test-exception.q",
            "program": "${workspaceFolder}/test/test-exception.q"
        },
        {
            "type": "qore",
            "request": "launch",
            "name": "Launch test-class.q",
            "program": "${workspaceFolder}/test/test-class.q"
        },
        {
            "type": "qore",
            "request": "launch",
            "name": "Launch test-closure.q",
            "program": "${workspaceFolder}/test/test-closure.q"
        },
        {
            "type": "qore",
            "request": "launch",
            "name": "Launch test-pause.q",
            "program": "${workspaceFolder}/test/test-pause.q"
        },
        {
            "type": "qore",
            "request": "attach",
            "name": "Attach test-debug_job:1.0@localhost:8001",
            "connection": "ws://localhost:8001/debug",
            "program": "test-debug_job:1.0"
        },
        {
            "type": "qore",
            "request": "launch",
            "name": "Test launch options",
            //"program": "${workspaceFolder}/${command:AskForProgramName}",
            "program": "/home/tma/work/qore/qore/test/qore/debug/test-debug-script.q",
            "logFilename": "/tmp/qvscdbg.log",
            "appendToLog": true,
            "fullException": true,
            "define": ["TEST_DEFINE_VALUE=test", "TEST_DEFINE"],
            "parseOptions": [
                "PO_NEW_STYLE",
            ],
            "timeZone": "1",
            "verbosity": 6,
            "stopOnEntry": true
        },
        {
            "type": "qore",
            "request": "launch",
            "name": "Ask for file name",
            "program": "${workspaceFolder}/${command:AskForProgramName}"
        }
    ]
}